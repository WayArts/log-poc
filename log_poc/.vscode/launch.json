        {
            "name": "Flutter Web",
            "request": "launch",
            "type": "dart",
            "deviceId": "chrome",
            "args": [
                "--web-renderer",
                "auto"
            ]
        },
        {
            "name": "Flutter Windows",
            "request": "launch",
            "type": "dart",
            "deviceId": "windows"
        },
        {
            "name": "log_poc",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "log_poc (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "log_poc (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
